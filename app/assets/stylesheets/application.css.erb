@font-face {
  font-family: 'OpenSansBold';
  src:url('<%= asset_path('OpenSansBold.eot') %>');
  src:url('<%= asset_path('OpenSansBold.woff') %>') format('woff'),
    url('<%= asset_path('OpenSansBold.ttf') %>') format('truetype');
  font-weight: normal;
  font-style: normal;
}

